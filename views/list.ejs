<html>
    <head>
        <title>Tasks To Be Done</title>
        <link rel="stylesheet" href="Style/styles.css">
    </head>
    <body>
        <div class="box" id="heading">
            <h1><%=day%></h1>
        </div>

        <div class="box">
           <% for(var i=0; i<another.length; i++){  %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%=another[i]%>
                </p>
            </div>
            <%   }  %>
        
        <form class="item" action="/" method="post">
            <input type="text" name="add" placeholder="New Item" autocomplete="off">
            <button type="submit">+</button>
        </form>
    </div>

    </body>
</html>